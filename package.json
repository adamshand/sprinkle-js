{
    "name": "sprinkle-js",
    "version": "0.1.7",
    "description": "A quick drop in reactive library to sprinkle reactivity in your web-app",
    "module": "dist/sprinkle-js.esm.js",
    "main": "dist/sprinkle-js.esm.js",
    "scripts": {
        "build": "node ./automation/revert-package-json && yarn preconstruct build && node ./automation/pack-browser.js",
        "publish:patch": "yarn build && yarn version --patch",
        "publish:minor": "yarn build && yarn version --minor",
        "publish:major": "yarn build && yarn version --major",
        "bump:patch": "yarn version --patch",
        "bump:minor": "yarn version --minor",
        "bump:major": "yarn version --major",
        "test": "jest --verbose",
        "coverage": "jest --coverage"
    },
    "repository": "git+https://github.com/paoloricciuti/sprinkle-js.git",
    "keywords": [
        "sprinkle",
        "javascript",
        "reactive",
        "dropin"
    ],
    "author": "Paolo Ricciuti",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/paoloricciuti/sprinkle-js/issues"
    },
    "homepage": "https://github.com/paoloricciuti/sprinkle-js#readme",
    "devDependencies": {
        "@babel/preset-env": "^7.18.2",
        "@babel/preset-typescript": "^7.17.12",
        "@preconstruct/cli": "^2.1.5",
        "@types/jest": "^27.5.1",
        "babel-jest": "^28.1.0",
        "babel-minify": "^0.5.2",
        "babel-preset-minify": "^0.5.2",
        "jest": "^28.1.0",
        "jest-environment-jsdom": "^28.1.0",
        "typescript": "^4.6.4"
    },
    "volta": {
        "node": "17.0.1"
    }
}
